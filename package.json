{
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@bazel/ibazel": "0.11.2",
    "@bazel/typescript": "1.2.0",
    "@types/react": "16.9.21",
    "@types/react-dom": "16.9.5",
    "typescript": "3.6.4",
    "concurrently": "3.5.1",
    "@bazel/bazel": "2.0.0",
    "@bazel/terser": "^1.3.0",
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "7.8.3",
    "@bazel/rollup": "^1.3.0",
    "express": "4.16.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup": "^1.21.4",
    "terser": "4.3.1"
  },
  "scripts": {
    "start-backend": "bazel run //src/backend",
    "start-frontend": "bazel build //src/frontend:bundle-es5.min",
    "build-backend": "/usr/bin/bazel-2.0.0 build //src/backend",
    "build-frontend": "/usr/bin/bazel-2.0.0 build //src/frontend:devserver",
    "start": "npm run start-backend",
    "start-remote-cache": "docker-compose -f caching/docker-compose.yml up",
    "stop-remote-cache": "docker-compose -f caching/docker-compose.yml down",
    "start-remote-server": "docker-compose -f remote-build/docker-compose-server.yml up",
    "start-remote-worker": "docker-compose -f remote-build/docker-compose-worker.yml up",
    "start-client": "docker-compose -f docker-compose-client.yml up --build"
  }
}